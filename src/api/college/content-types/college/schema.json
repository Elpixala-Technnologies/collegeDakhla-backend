{
  "kind": "collectionType",
  "collectionName": "colleges",
  "info": {
    "singularName": "college",
    "pluralName": "colleges",
    "displayName": "college",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "url": {
      "type": "string"
    },
    "college_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::college-type.college-type"
    },
    "collegeName": {
      "type": "string",
      "unique": true
    },
    "collegeLogo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "establishmentYear": {
      "type": "string"
    },
    "rankedBy": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ranking-body.ranking-body",
      "mappedBy": "college"
    },
    "approvedBy": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::organization.organization",
      "mappedBy": "college"
    },
    "collegeDescription": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "collegeStreams": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::stream.stream",
      "mappedBy": "streams"
    },
    "city": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::city.city"
    },
    "brochure": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "banner": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "isTopCollege": {
      "type": "boolean",
      "default": false
    },
    "pincode": {
      "type": "string"
    },
    "state": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::state.state",
      "inversedBy": "colleges"
    },
    "country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::country.country",
      "inversedBy": "colleges"
    }
  }
}
