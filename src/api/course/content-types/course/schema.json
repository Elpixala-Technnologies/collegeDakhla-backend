{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "isTopCourse": {
      "type": "boolean",
      "default": false
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "banner": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "name": {
      "type": "string"
    },
    "streams": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::stream.stream",
      "inversedBy": "courses"
    },
    "courseData": {
      "type": "component",
      "repeatable": true,
      "component": "page-data.data"
    },
    "navbars": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::navbar.navbar",
      "inversedBy": "courses"
    },
    "college_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::college-type.college-type",
      "inversedBy": "course"
    },
    "specializations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::specialization.specialization",
      "inversedBy": "courses"
    },
    "course_levels": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course-level.course-level",
      "inversedBy": "courses"
    }
  }
}
